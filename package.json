{
  "name": "RiverChurch",
  "description": "An amazing, rich, isomorphic website created with skookum-react",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "lint": "npm run eslint && npm run suit",
    "eslint": "eslint .",
    "suit": "node scripts/suit-conformance",
    "start": "DEBUG=*,-babel node . & node server.js",
    "test": "npm run lab && npm run jest",
    "test-client": "jest",
    "test-server": "lab -T node_modules/lab-babel -t 100 -S",
    "build": "node ./scripts/build && node dist/main.js"
  },
  "dependencies": {
    "axios": "^0.5.4",
    "btoa": "^1.1.2",
    "es6-promise": "^2.3.0",
    "flux": "^2.0.3",
    "hapi": "^8.6.1",
    "hapi-mailer": "2.0.0",
    "hapi-react-views": "iamdustan/hapi-react-views#master",
    "immutable": "^3.7.4",
    "isomorphic-fetch": "^2.1.0",
    "joi": "^6.5.0",
    "json-loader": "^0.5.2",
    "moment": "^2.10.6",
    "node-noop": "0.0.1",
    "normalize.css": "^3.0.3",
    "object-assign": "^3.0.0",
    "react": "^0.14.0-beta.3",
    "react-dom": "^0.14.0-beta3",
    "react-resolver": "iamdustan/react-resolver#duckpunch-dist",
    "react-router": "1.0.0-beta3",
    "source-map-support": "^0.3.2",
    "suitcss-preprocessor": "^0.4.0",
    "xml2js": "^0.4.9"
  },
  "devDependencies": {
    "babel-core": "^5.6.14",
    "babel-eslint": "^3.1.19",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.2.2",
    "babel-runtime": "^5.6.14",
    "chalk": "^1.0.0",
    "code": "^1.4.1",
    "css-loader": "^0.15.1",
    "debug": "^2.2.0",
    "eslint": "^0.24.0",
    "eslint-plugin-react": "^2.5.2",
    "esprima-loader": "^0.9.0",
    "express": "^4.13.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "glob": "^5.0.10",
    "jest-cli": "^0.4.13",
    "lab": "^5.12.0",
    "lab-babel": "^1.0.1",
    "react-hot-loader": "^1.2.8",
    "rework": "^1.0.1",
    "rework-suit-conformance": "^0.4.2",
    "rework-vars": "^3.1.1",
    "rework-webpack-loader": "^0.2.0",
    "source-map": "^0.4.2",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.12",
    "webpack-dev-server": "^1.9.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest-preprocessor",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testFileExtensions": [
      "js"
    ]
  }
}
